{
  "name": "@sylphlab/zen-query-preact",
  "version": "0.1.3",
  "description": "Preact hooks for TypeQL",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@sylphlab/zen-query-client": "workspace:*",
    "@sylphlab/zen-query-shared": "workspace:*"
  },
  "peerDependencies": {
    "preact": "^10.5.0"
  },
  "devDependencies": {
    "@preact/signals-core": "^1.6.2",
    "@testing-library/preact": "^3.2.3",
    "@types/node": "^20.14.2",
    "happy-dom": "^14.12.3",
    "preact": "^10.22.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@sylphlab/prettier-config",
  "license": "MIT"
}