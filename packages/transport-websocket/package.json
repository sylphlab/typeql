{
  "name": "@sylphlab/typeql-transport-websocket",
  "version": "0.0.1",
  "description": "WebSocket transport adapter for TypeQL",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format esm,cjs --external ws @sylph/typeql-shared --watch",
    "clean": "rimraf --glob dist \"*.tsbuildinfo\"",
    "test": "vitest run"
  },
  "keywords": [
    "typeql",
    "websocket",
    "transport",
    "api",
    "rpc",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sylph/typeql-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/ws": "^8.5.0",
    "typescript": "^5.0.0",
    "ws": "^8.10.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
