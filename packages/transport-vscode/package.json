{
  "name": "@sylphlab/typeql-transport-vscode",
  "version": "0.0.2",
  "description": "TypeQL transport adapter for VSCode Extension communication (postMessage)",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm exec tsup",
    "dev": "tsup src/index.ts --format esm,cjs --external @sylph/typeql-shared --watch",
    "clean": "rimraf --glob dist \"*.tsbuildinfo\"",
    "test": "vitest run"
  },
  "keywords": [
    "typeql",
    "typescript",
    "rpc",
    "realtime",
    "vscode",
    "transport"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@sylph/typeql-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@types/vscode": "^1.80.0"
  }
,
  "publishConfig": {
    "access": "public"
  }
}