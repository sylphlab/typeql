{
  "name": "@sylphlab/zen-query-client",
  "version": "0.0.4",
  "description": "Client library for TypeQL - TypeScript-first realtime API framework.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "typeql",
    "client",
    "rpc",
    "typescript",
    "realtime",
    "state-sync"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sylphlab/zen-query-shared": "workspace:*",
    "fast-json-patch": "^3.1.1",
    "immer": "^10.1.1",
    "nanoevents": "^9.1.0",
    "nanoid": "^5.1.5",
    "nanostores": "^1.0.1",
    "zod": "^3.24.2"
  }
}