{
  "name": "@sylphlab/typeql-react",
  "version": "0.0.1",
  "description": "React integration for TypeQL",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm exec tsup",
    "dev": "tsup src/index.ts --format esm,cjs --external react @sylphlab/typeql-shared @sylphlab/typeql-client --watch",
    "clean": "rimraf --glob dist \"*.tsbuildinfo\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "typeql",
    "react",
    "hooks",
    "api",
    "rpc",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@sylphlab/typeql-client": "workspace:*",
    "@sylphlab/typeql-shared": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
